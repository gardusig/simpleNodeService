{
    "compilerOptions": {
        "module": "commonjs",
        "declaration": true,
        "removeComments": true,
        "emitDecoratorMetadata": true,
        "strict": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "target": "ES2021",
        "sourceMap": true,
        "incremental": true,
        "skipLibCheck": true,
        "baseUrl": "./",
        "outDir": "dist/",
    },
    "exclude": [
        "node_modules/**/*",
        "dist/**/*"
    ],
}