{
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "build": "nest build",
        "start:random-object-service": "node dist/user-service/server/user.main.js",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "lint": "eslint './**/*.ts'",
        "format": "eslint './**/*.ts' --fix",
        "prisma:deploy": "npx prisma migrate deploy --schema ./prisma/database/schema.prisma",
        "prisma:generate": "npx prisma generate --schema ./prisma/database/schema.prisma",
        "prisma:migrate": "npx prisma migrate dev --schema ./prisma/database/schema.prisma --name migration --skip-seed",
        "prisma:reset": "npx prisma migrate reset --schema ./prisma/database/schema.prisma --force --skip-seed",
        "prisma:start": "npm run prisma:deploy && npm run prisma:generate"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "testEnvironment": "node",
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testRegex": ".*\\.spec\\.ts$",
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ]
    },
    "dependencies": {
        "@nestjs/common": "^10.2.0",
        "@nestjs/config": "^3.1.1",
        "@nestjs/core": "^10.4.4",
        "@nestjs/platform-express": "^10.3.7",
        "@nestjs/swagger": "^7.4.2",
        "@nestjs/terminus": "^10.2.3",
        "@prisma/client": "^5.20.0",
        "@vendia/serverless-express": "^4.12.6",
        "aws-lambda": "^1.0.7",
        "axios": "1.7.4",
        "axios-mock-adapter": "^2.0.0",
        "axios-retry": "^4.5.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "jest": "29.1.1",
        "js-yaml": "^4.1.0",
        "jsonwebtoken": "^9.0.2",
        "jwt-decode": "^4.0.0",
        "npm": "^10.5.2",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "supertest": "6.3.3",
        "swagger-ui-express": "^5.0.0",
        "ts-jest": "29.1.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.4.5",
        "@nestjs/schematics": "^10.0.3",
        "@nestjs/testing": "^10.4.4",
        "@types/aws-lambda": "^8.10.145",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/js-yaml": "^4.0.9",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/node": "^20.8.7",
        "@types/supertest": "^2.0.16",
        "@types/swagger-ui-express": "^4.1.6",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-import-helpers": "^1.3.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-unused-imports": "^4.1.4",
        "install": "^0.13.0",
        "prettier": "^3.2.4",
        "prettier-eslint": "^16.3.0",
        "prisma": "^5.8.1",
        "rimraf": "^5.0.5",
        "ts-loader": "^9.5.1",
